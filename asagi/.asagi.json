{"settings": {
  "dumperEngine": "DumperJSON",
  "sourceEngine": "YotsubaJSON",

  "boardSettings": {
    "default": {
      "engine": "${ASAGI_ENGINE}",
      "database": "${ASAGI_DB}",
      "host": "${ASAGI_DB_HOST}",
      "username": "${ASAGI_DB_USERNAME}",
      "password": "${ASAGI_DB_PASSWORD}",
      "charset": "${ASAGI_DB_CHARSET}",
      "path": "${ASAGI_DL_PATH}",
      "useOldDirectoryStructure": ${ASAGI_OLD_DIR_STRUCT},
      "webserverGroup": "${ASAGI_WEBSERVER_GROUP}",
      "thumbThreads": ${ASAGI_THUMB_THREAD_NUM},
      "mediaThreads": ${ASAGI_MEDIA_THREAD_NUM},
      "newThreadsThreads": ${ASAGI_NEW_THREADS_THREADS},
      "deletedThreadsThresholdPage": ${ASAGI_DEL_THREAD_THRESH_PAGE},
      "refreshDelay": ${ASAGI_REFRESH_DELAY},
      "throttleAPI": ${ASAGI_THROT_API},
      "throttleURL": "${ASAGI_THROT_URL}",
      "throttleMillisec": ${ASAGI_THROT_MILLSEC},

      "threadRefreshRate": ${ASAGI_REFRESH_RATE},
      "pageSettings": [
        {"delay": 30, "pages": [0, 1]},
        {"delay": 500, "pages": [2, 3, 4, 5, 6, 7, 8]},
        {"delay": 30, "pages": [9, 10]}
      ]
    },

    ${ASAGI_THREADS}
  }
}}
