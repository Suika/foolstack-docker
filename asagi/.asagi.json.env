{"settings": {
  "dumperEngine": "DumperJSON",
  "sourceEngine": "YotsubaJSON",

  "boardSettings": {
    "default": {
      "engine":     "${SCRAPER_DB_ENGINE}",
      "database":   "${SCRAPER_DB_NAME}",
      "host":       "${SCRAPER_DB_HOST}",
      "username":   "${SCRAPER_DB_USER}",
      "password":   "${SCRAPER_DB_PASS}",
      "charset":    "${SCRAPER_DB_CHARSET}",
      "path":       "${SCRAPER_IMGDIR}",
      "useOldDirectoryStructure":   ${SCRAPER_ASAGI_OLDSTRUCT},
      "webserverGroup":             "${SCRAPER_ASAGI_WEBGRP}",
      "thumbThreads":               ${SCRAPER_THUMB_THREADS},
      "mediaThreads":               ${SCRAPER_MEDIA_THREADS},
      "newThreadsThreads":          ${SCRAPER_THREAD_THREADS},
      "deletedThreadsThresholdPage":${SCRAPER_DEL_THREAD_THRESH_PAGE},
      "refreshDelay":               ${SCRAPER_BORADS_DELAY},
      "throttleAPI":                ${SCRAPER_API_THROTTLE},
      "throttleURL":                "${SCRAPER_API_URL}",
      "throttleMillisec":           ${SCRAPER_API_DELAY},
      "threadRefreshRate":          ${SCRAPER_REFRESH_DELAY},
      "pageSettings": [
        {"delay": 30, "pages": [0, 1]},
        {"delay": 500, "pages": [2, 3, 4, 5, 6, 7, 8]},
        {"delay": 30, "pages": [9, 10]}
      ]
    },

    ${SCRAPER_BOARDS}
  }
}}
